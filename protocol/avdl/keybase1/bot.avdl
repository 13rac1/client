
@namespace("keybase.1")
protocol bot {

  import idl "common.avdl";

  @typedef("string") record BotToken {}

  record BotTokenInfo {
    BotToken token;
    Time ctime;
  }

  enum UserType {
     HUMAN_0,
     BOT_1
  }

  array<BotTokenInfo> botTokenList();
  BotToken botTokenCreate();
  void botTokenDelete(BotToken token);

}
